{
    "name": "star-insure/api-client",
    "description": "Star Insure API Client",
    "keywords": [
        "star-insure"
    ],
    "homepage": "https://github.com/star-insure/api-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alex Clark",
            "email": "alex@intuitcreative.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "funkjedi/composer-include-files": "^1.1",
        "illuminate/support": "^8.0 | ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "StarInsure\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StarInsure\\Api\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "StarInsure\\Api\\Providers\\StarAuthServiceProvider",
                "StarInsure\\Api\\Providers\\StarApiServiceProvider"
            ],
            "aliases": {
                "StarAuth": "StarInsure\\Api\\Facades\\StarAuth",
                "StarApi": "StarInsure\\Api\\Facades\\StarApi"
            }
        }
    }
}
